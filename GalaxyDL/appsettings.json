{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "GalaxyDL": "Debug"
    }
  },
  "GogApi": {
    "DefaultTimeout": 30,
    "DownloadTimeout": 300,
    "MaxRetries": 3,
    "ChunkSize": 104857600
  },
  "Archive": {
    "DefaultArchiveRoot": "./archive",
    "MaxConcurrentDownloads": 4,
    "EnableIntegrityChecking": true,
    "SavePrettifiedManifests": true
  }
}